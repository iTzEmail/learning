{
  "hosting": {
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/",
        "destination": "/home.html"
      },
      {
        "source": "/home",
        "destination": "/home.html"
      }
    ],
    "errorPage": [
      {
        "code": 404,
        "location": "/404"
      }
    ]
  }
}